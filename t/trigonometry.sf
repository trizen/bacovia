#!/usr/bin/ruby

include('../lib/bacovia.sf')

assert_eq(cos(Log(Symbol('x'))).simplify, Fraction(Sum(Power(Symbol('x'), -1i), Power(Symbol('x'), 1i)), 2))
assert_eq(sin(Log(Symbol('x'))).simplify, Fraction(Sum(Power(Symbol('x'), 1i), Product(Power(Symbol('x'), -1i), -1)), 2i))

say "** Test passed!"
