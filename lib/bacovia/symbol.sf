#!/usr/bin/ruby

class Symbol(name) {

    method neg {
        Product(-1, self)
    }

    method inv {
        Fraction(1, self)
    }

    method alternatives {
        [self]
    }

    method ==(Symbol o) {
        name == o.name
    }

    method ==(_) {
        false
    }

    method pretty {
        "#{name}"
    }

    method to_s {
        "Symbol('#{name}')"
    }
}
